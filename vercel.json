{
    "builds": [
        {
        "src": "JVCorp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": { 
            "distDir": "staticfiles_build"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "JVCorp/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "JVCorp/wsgi.py"
        }
    ]
}